using System.Collections;
using System.Collections.Generic;
using UnityEngine;
using UnityEngine.Events;

public class Switch_Door : MonoBehaviour
{
    public bool _open;
    private void OnTriggerEnter2D(Collider2D collision)
    {
        if(Input.GetKeyDown(KeyCode.F))
        {
            _open = true;
        }
    }
}
